{
	"name": "@terrygonguet/utils",
	"version": "0.0.10",
	"type": "module",
	"module": "./dist/index.js",
	"author": {
		"email": "terry@gonguet.com",
		"name": "Terry Gonguet",
		"url": "https://terry.gonguet.com/"
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"test": "vitest test --ui --watch --update",
		"coverage": "vitest run --coverage"
	},
	"devDependencies": {
		"@vitest/coverage-v8": "^0.32.0",
		"@vitest/ui": "^0.32.0",
		"prettier": "^2.8.8",
		"typescript": "^5.0.2",
		"vite": "^4.3.9",
		"vitest": "^0.32.0"
	},
	"files": [
		"dist/**/*.js",
		"src/**/!(*.test).ts",
		"types/**/!(*.test).d.ts"
	],
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./async": {
			"import": "./dist/async.js"
		},
		"./functional": {
			"import": "./dist/functional.js"
		}
	},
	"typesVersions": {
		"*": {
			"async": [
				"./types/async.d.ts"
			],
			"functional": [
				"./types/functional/index.d.ts"
			]
		}
	},
	"types": "./types/index.d.ts",
	"dependencies": {
		"just-compose": "^2.3.0",
		"just-pipe": "^1.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/terrygonguet/utils.git"
	},
	"bugs": {
		"url": "https://github.com/terrygonguet/utils/issues"
	}
}
